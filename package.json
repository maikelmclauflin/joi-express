{
  "name": "joi-express",
  "version": "1.0.3",
  "description": "Simple validation middleware for express using the joi validation suite.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "coverage",
      "test"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "dependencies": {
    "boom": "7.2.0",
    "extend": "3.0.2",
    "joi": "13.x.x"
  },
  "devDependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  },
  "peerDependencies": {
    "joi": "13.x.x"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc --check-coverage --lines 95 --functions 100 --branches 100 --statements 95 mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vforv/joi-express.git"
  },
  "keywords": [
    "middleware",
    "joi",
    "validation",
    "express"
  ],
  "author": "Vladimir Djukic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vforv/joi-express/issues"
  },
  "homepage": "https://github.com/vforv/joi-express#readme"
}
